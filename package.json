{
  "name": "point-of-vue",
  "version": "0.0.1",
  "description": "state devtools for Vue and the Composition API",
  "main": "dist/index.js
  "repository": "https://github.com/oslabs-beta/point-of-vue",
  "author": "Point of Vue ",
  "contributors": [
    "Shelby Neuman",
    "Tony Lei",
    "Tristen Francis",
    "Will Robson"
  ],
  "license": "MIT",
  "module": "dist/point-of-vue.mjs",
  "unpkg": "dist/point-of-vue.iife.js",
  "jsdelivr": "dist/point-of-vue.iife.js",
  "exports": {
    ".": {
      "browser": "./dist/point-of-vue.esm-browser.js",
      "node": {
        "import": {
          "production": "./dist/point-of-vue.prod.cjs",
          "development": "./dist/point-of-vue.mjs",
          "default": "./dist/point-of-vue.mjs"
        },
        "require": {
          "production": "./dist/point-of-vue.prod.cjs",
          "development": "./dist/point-of-vue.cjs",
          "default": "./index.js"
        }
      },
      "import": "./dist/point-of-vue.mjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve public"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "@vue/devtools-api": "^6.1.4",
    "date-fns": "^2.16.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "vue-demi": "*"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.0",
    "typescript": ">=4.4.4",
    "vue": "^3.2.36"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.19.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.36.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "serve": "^11.3.2",
    "typescript": "4.7.2",
    "vue": "^3.2.36"
  }
}
